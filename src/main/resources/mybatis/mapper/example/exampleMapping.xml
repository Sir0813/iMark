<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
"http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.dm.frame.jboot.example.mapper.ExampleMapper">
    <!-- 查询所有 -->
    <select id="findAll" resultType="Example">
        SELECT fid,fname FROM tb_example 
    </select>
    
    <!-- 分页查询 -->
    <select id="findByPage" resultType="Example">
        SELECT fid,fname FROM tb_example 
    </select>

    <insert id="insertExample" parameterType="Example" useGeneratedKeys="true" keyProperty="fid">
        INSERT INTO tb_example(fname) VALUES (#{fname}) 
    </insert>
</mapper>
